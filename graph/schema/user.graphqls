type User {
  id: ID!
  name: String!
  lastName: String
}

input NewUser {
  name: String
  lastName: String
}

extend type Query {
	users: [User!]!
  findUserByName(name: String!): [User!]!
}

extend type Mutation {
  createUser(input: NewUser!): User!
  updateUser(id: ID!, body: NewUser!): Boolean!
}